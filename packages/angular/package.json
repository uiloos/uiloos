{
  "name": "@automata.dev/angular",
  "version": "0.0.0",
  "description": "The Angular bindings of the automata headless UI",
  "peerDependencies": {
    "@angular/common": "^13.2.0",
    "@angular/core": "^13.2.0"
  },
  "dependencies": {
    "@automata.dev/core": "0.0.0"
  },
  "devDependencies": {
		"@types/estree": "0.0.51",
    "@types/jasmine": "3.10.3",
    "@angular/cli": "13.2.3",
    "ng-packagr": "13.2.1",
    "@angular/common": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular-devkit/build-angular": "13.2.3",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@testing-library/angular": "11.0.2",
    "typescript": "4.5.5",
    "karma": "6.3.16",
    "karma-jasmine": "4.0.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "karma-coverage": "2.2.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf coverage && rm -f .package.lock-json && rm -rf dist",
