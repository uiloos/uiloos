{
  "name": "@uiloos/angular",
  "version": "0.0.5",
  "description": "The Angular bindings of the uiloos headless UI",
  "keywords": [
    "uiloos"
  ],
  "author": "Maarten Hus",
  "homepage": "https://uiloos.dev",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/fesm2015/uiloos-angular.mjs",
  "es2020": "dist/fesm2020/uiloos-angular.mjs",
  "esm2020": "dist/esm2020/uiloos-angular.mjs",
  "fesm2020": "dist/fesm2020/uiloos-angular.mjs",
  "fesm2015": "dist/fesm2015/uiloos-angular.mjs",
  "typings": "dist/uiloos-angular.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@angular/common": "^13.2.0",
    "@angular/core": "^13.2.0"
  },
  "dependencies": {
    "@uiloos/core": "^0.0.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.7",
    "@angular/animations": "13.3.10",
    "@angular/cli": "13.3.7",
    "@angular/common": "13.3.10",
    "@angular/core": "13.3.10",
    "@angular/platform-browser-dynamic": "13.3.10",
    "@testing-library/angular": "11.0.4",
    "@types/estree": "0.0.51",
    "@types/jasmine": "4.0.3",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "13.3.1",
    "typedoc": "0.22.15",
    "typescript": "4.6.4"
  },
  "overrides": {
    "typescript": "4.6.4"
  },
  "scripts": {
    "before-publish": "cp ../../README.MD ./ && cp ../../LICENSE.MD dist && rm dist/package.json",
    "clean": "rm -rf node_modules && rm -rf coverage && rm -f package-lock.json && rm -rf dist",
    "test": "ng test --code-coverage=true --watch=false",
    "start": "ng test --code-coverage=true",
    "tsc": "tsc --version && tsc --noEmit",
    "build": "rm -rf dist && ng-packagr -p ng-package.json",
    "apigen": "rm -rf apigen && typedoc src/public-api.ts  --json ../website/src/_data/api/angular.json"
  }
}
