{
	"name": "@uiloos/svelte",
  "version": "1.2.1",
	"scripts": {
		"before-publish": "cp ../../README.MD ./ && cp ../../LICENSE.MD dist",
		"start": "svelte-kit sync && vitest watch --coverage",
		"clean": "rm -rf coverage && rm -rf dist",
		"test": "svelte-kit sync && vitest run",
		"tsc": "tsc --version && tsc --noEmit",
		"build": "svelte-kit sync && vite build && svelte-package",
		"apigen": "typedoc src/lib/index.js --json ../website/src/_data/api/svelte.json"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"dependencies": {
		"@uiloos/core": "^1.2.1"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.5",
		"@sveltejs/package": "2.1.0",
		"@vitest/coverage-v8": "0.32.2",
		"jsdom": "22.1.0",
		"svelte": "4.0.0",
		"tslib": "2.6.0",
		"typedoc": "0.24.8",
		"typescript": "5.1.5",
		"vite": "4.3.9",
		"vitest": "0.32.2"
	},
	"overrides": {
		"typescript": "5.1.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
