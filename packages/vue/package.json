{
  "name": "@uiloos/vue",
  "version": "0.0.5",
  "description": "The Vue bindings of the uiloos headless UI",
  "keywords": [
    "uiloos"
  ],
  "author": "Maarten Hus",
  "homepage": "https://uiloos.dev",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "before-publish": "cp ../../README.MD ./ && cp ../../LICENSE.MD dist",
    "start": "vitest watch --coverage",
    "clean": "rm -rf node_modules && rm -rf coverage && rm -f package-lock.json && rm -rf dist",
    "test": "vitest run --coverage",
    "tsc": "tsc --version && tsc --noEmit",
    "build": "rm -rf dist && vite build && tsc --emitDeclarationOnly --outfile dist/index",
    "apigen": "rm -rf apigen && typedoc src/index.ts --json ../website/src/_data/api/vue.json"
  },
  "dependencies": {
    "@uiloos/core": "^0.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "6.6.1",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitest/coverage-c8": "0.22.1",
    "c8": "7.12.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "typedoc": "0.23.10",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.22.1",
    "vue": "3.2.37"
  },
  "overrides": {
    "typescript": "4.7.4"
  },
  "gitHead": "0be49f92c7e4ea9970be2610af79eb06e8d9477d"
}
