{
  "name": "@uiloos/vue",
  "version": "1.2.0",
  "description": "The Vue bindings of the uiloos headless UI",
  "keywords": [
    "uiloos"
  ],
  "author": "Maarten Hus",
  "homepage": "https://uiloos.dev",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "before-publish": "cp ../../README.MD ./ && cp ../../LICENSE.MD dist",
    "start": "vitest watch --coverage",
    "clean": "rm -rf node_modules && rm -rf coverage && rm -f package-lock.json && rm -rf dist",
    "test": "vitest run --coverage",
    "tsc": "tsc --version && tsc --noEmit",
    "build": "rm -rf dist && vite build && tsc --emitDeclarationOnly --outfile dist/index",
    "apigen": "rm -rf apigen && typedoc src/index.ts --json ../website/src/_data/api/vue.json"
  },
  "dependencies": {
    "@uiloos/core": "^1.2.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "7.0.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-v8": "0.32.2",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "typedoc": "0.22.15",
    "typescript": "4.7.4",
    "vite": "4.3.9",
    "vitest": "0.32.2",
    "vue": "3.3.4"
  },
  "overrides": {
    "typescript": "4.7.4"
  },
  "gitHead": "0be49f92c7e4ea9970be2610af79eb06e8d9477d"
}
