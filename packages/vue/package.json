{
  "name": "@uiloos/vue",
  "version": "0.0.1",
  "description": "The Vue bindings of the uiloos headless UI",
  "keywords": [
    "uiloos"
  ],
  "author": "Maarten Hus",
  "homepage": "https://uiloos.dev",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "before-publish": "cp ../../README.MD dist && cp ../../LICENSE.MD dist",
    "start": "vitest watch --coverage",
    "clean": "rm -rf node_modules && rm -rf coverage && rm -f .package.lock-json && rm -rf dist",
    "test": "vitest run --coverage",
    "tsc": "tsc --version && tsc --noEmit",
    "build": "rm -rf dist && vite build && tsc --emitDeclarationOnly --outfile dist/index",
    "apigen": "rm -rf apigen && typedoc src/index.ts --json ../website/src/_data/api/vue.json"
  },
  "dependencies": {
    "@uiloos/core": "^0.0.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "next",
    "@vitejs/plugin-vue": "2.2.0",
    "c8": "7.11.0",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "typedoc": "0.22.12",
    "typescript": "4.5.5",
    "vite": "2.8.2",
    "vitest": "0.3.5",
    "vue": "3.2.31"
  }
}
