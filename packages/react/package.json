{
  "name": "@uiloos/react",
  "version": "1.5.0",
  "description": "The React bindings of the uiloos headless UI",
  "keywords": [
    "uiloos"
  ],
  "author": "Maarten Hus",
  "homepage": "https://uiloos.dev",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "before-publish": "cp ../../README.MD ./ && cp ../../LICENSE.MD dist",
    "clean": "rm -rf coverage && rm -rf dist",
    "test": "jest --coverage",
    "start": "jest --watch --coverage",
    "tsc": "tsc --version && tsc --noEmit",
    "build": "rm -rf dist && rollup -c",
    "apigen": "typedoc src  --json ../website/src/_data/api/react.json"
  },
  "dependencies": {
    "@uiloos/core": "^1.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.1.0"
