---
layout: layouts/base.njk
permalink: "/docs/{{example.url}}/examples/{{example.framework | slug}}"
stickyNavigation: false
pagination:
  data: examples
  size: 1
  alias: example
eleventyComputed:
  title: "{{ example.text }}"
  description: "{{ example.description}}"
  keywords: "{{example.keywords}}"
---

<section class="max-w-6xl mx-auto px-5 sm:px-6 mt-8">

  <nav aria-label="Breadcrumb" class='breadcrumb'>
    <ol>
      <li>
        <a href="../../">
          Docs
        </a>
      </li>
      <li>
        <a href="../" class="capitalize">
          {{example.component}}
        </a>
      </li>
      <li>
        <a href="../examples" class="capitalize">
          examples
        </a>
      </li>
      <li>
        <a href="" aria-current="page" class="font-bold">
          {{example.framework}}
        </a>
      </li>
    </ol>
  </nav>

  <article>
    <h1>{{example.text}}</h1>

    <p>
      {{example.description}}
    </p>

    <p class="mt-4">
      <a href="https://www.github.com{{example.github}}" class="text-purple-600 underline">View code on GitHub</a>
    </p>
  </article>
</section>

<iframe src="https://stackblitz.com/github{{example.github}}?preset=node&embed=1&showSideBar=1" style="width: 100%; height: 800px;"></iframe>
