{% from "../_includes/displayApi.njk" import displayApi %}

{% macro displayApiGroup(definitions, groups = []) %}
  <ol>
    {% for group in groups %}
      <li>
        <h2 id="{{group}}"><a href="#{{group}}">{{loop.index}}. {{group}}</a></h3>

        {{ displayApi(definitions | groupApi(groups, group)) }}
      </li>
    {% endfor %}
  <ol>
{% endmacro %}

<article>
  <h1>Core</h1>

  <p>The core of uiloos consists of pure JavaScript classes and functions.</p>
  
  {{ displayApiGroup(distilled.core, ['ActiveList', 'ViewChannel', 'Typewriter', 'DateGallery', 'License', 'Generic']) }}

</article>