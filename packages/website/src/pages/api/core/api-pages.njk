---
layout: layouts/api.njk
permalink: "/api/core/{{ def.name | slug }}/"
pagination:
  data: distilled.core
  size: 1
  alias: def
eleventyComputed:
  title: "{{ def.name }} | core | API" 
  description: "{{ def.name }} {{def.description}}"
  keywords: "@uiloos/core {{ def.name }} API, {{ def.name }}"
---

{% from "../_includes/error.njk" import errorPage %}
{% from "../_includes/class.njk" import classPage %}
{% from "../_includes/function.njk" import functionPage %}
{% from "../_includes/typealias.njk" import typealiasPage %}
{% from "../_includes/instance.njk" import instancePage %}

{% if def.kindString === 'error' %}
  {{ errorPage(def) }}
{% elif def.kindString === 'class' %}
  {{ classPage(def) }}
{% elif def.kindString === 'function' %}
  {{ functionPage(def) }}
{% elif def.kindString === 'type' %}
  {{ typealiasPage(def) }}
{% elif def.kindString === 'instance' %}
  {{ instancePage(def) }}
{% endif %}

