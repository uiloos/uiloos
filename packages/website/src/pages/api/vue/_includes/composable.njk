{% from "../../_includes/macros.njk" import displaySignatures %}

<article>
  <h1>
    {{def.name}}

    {% if def.signatures[0].typeParameter.length > 0 %}
      &lt;{{def.signatures[0].typeParameter[0].name}}&gt;
    {% endif %}
  </h1>

  <p class="text-2xl font-mono mb-4 text-red-800">COMPOSABLE</p>
  
  {{ displaySignatures(def.signatures, 'vue') }}

</article>