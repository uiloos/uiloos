{
  "name": "website",
  "description": "The uiloos website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf _site",
    "serve:webpack": "webpack --mode development --watch",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve --config=./eleventy.config.js",
    "start": "npm-run-all apigen:post clean --parallel serve:*",
    "build-docs:webpack": "webpack --mode production",
    "build-docs:eleventy": "ELEVENTY_ENV=production eleventy --config=./eleventy.config.js",
    "build-docs:sitemap": "node ./generate-sitemap.js",
    "build-docs": "NODE_ENV=production run-s clean build-docs:*",
    "build": "echo ignoring build for website package",
    "distil-api": "node ./distil-api.js",
    "apigen:pre": "rm -rf src/_data/api",
    "apigen:post": "node ./distil-api.js && node ./generate-search-data.js ",
    "tsc": "tsc --version && tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@uiloos/core": "^1.6.0",
    "alpinejs": "3.13.7",
    "date-fns": "3.6.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@types/alpinejs": "3.7.2",
    "autoprefixer": "10.4.14",
    "css-loader": "6.8.1",
    "mini-css-extract-plugin": "2.7.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "postcss-loader": "7.3.3",
    "sass": "1.63.6",
    "sass-loader": "13.3.2",
    "tailwindcss": "3.3.2",
    "ts-loader": "9.4.3",
    "typescript": "5.1.5",
    "webpack": "5.88.0",
    "webpack-cli": "5.1.4"
  }
}
