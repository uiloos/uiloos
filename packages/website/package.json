{
  "name": "website",
  "description": "The uiloos website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && rm -rf _site",
    "clean:site": "rm -rf _site",
    "serve:webpack": "webpack --mode development --watch",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve --config=./eleventy.config.js",
    "start": "npm-run-all clean:site --parallel serve:*",
    "build-docs:webpack": "webpack --mode production",
    "build-docs:eleventy": "ELEVENTY_ENV=production eleventy --config=./eleventy.config.js",
    "build-docs": "NODE_ENV=production run-s clean:site build-docs:*",
    "build": "echo ignoring build for website package",
    "apigen": "rm -rf src/_data/api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "alpinejs": "3.10.2",
    "aos": "3.0.0-beta.6",
    "@uiloos/core": "^0.0.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.1",
    "@11ty/eleventy-navigation": "0.3.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.0.0",
    "@tailwindcss/forms": "0.5.2",
    "autoprefixer": "10.4.7",
    "css-loader": "6.7.1",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.0",
    "sass": "1.53.0",
    "sass-loader": "13.0.0",
    "tailwindcss": "3.1.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
